   "source": [
    "import cirq"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "q = cirq.GridQubit(0, 0)\n",
    "\n",
    "circuit = cirq.Circuit(\n",
    "    cirq.H(q),\n",
    "    cirq.measure(q, key='m')\n",
    ")\n",
    "print(circuit)\n",
    "\n",
    "sim = cirq.Simulator()\n",
    "res = sim.run(circuit, repetitions=100)\n",
    "res"
   ]
  },
